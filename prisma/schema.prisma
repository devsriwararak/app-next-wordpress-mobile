generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model down {
  id   Int    @id @default(autoincrement())
  name String @unique
  sum  Int
}

model mount {
  id   Int    @id @default(autoincrement())
  name String @unique
  sum  Int
}

model deposit_price {
  id   Int    @id @default(autoincrement())
  product_id Int @unique
  product_name String
  price  Int @default(0)
}
